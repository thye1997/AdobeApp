
<div class="bg-white ml-3 mr-3 mb-3 contact-container">
    <div class="container-fluid ml-3 mr-3 mt-3">
        <div class="mb-3" id="btnContainer">
            <button class="btn active" id="listViewBtn"><i class="fa fa-bars"></i> List</button>
            <button class="btn" id="gridViewBtn"><i class="fa fa-th-large"></i> Grid</button>
        </div>
        <div class="row no-gutters" id="outer-row" style="margin-right:inherit;">
            @for (int i = 0; i < 6; i++)
            {
                <a href="@i" class="card col-xl-8 col-md-12 co-sm-12 col-12  mb-3 no-gutters" style="text-decoration:none; color:inherit;">
                    <div class="card-body" style="font-size:12px; font-weight:400;">
                        <div class="no-padding row align-items-center">
                            <img class="profile-img mr-2" src="https://image.shutterstock.com/image-photo/profile-picture-smiling-millennial-asian-260nw-1836020740.jpg" style="width:55px;height:45px;" alt="name" />
                            <div class="profile-name col-lg-auto col-md-auto col-sm-auto col-auto font-weight-bold">Nicholas Gordon</div>
                            <div class="profile-email col-lg-auto col-md-auto col-sm-auto col-auto">chenthye0825@hotmail.com</div>
                            <div class="profile-hpNumber col-lg-auto col-md-auto col-sm-auto col-auto">018-9678543</div>
                            <div class="profile-hourlyRate mr-3 col-lg-auto col-md-auto col-sm-auto col-auto"><span style="font-weight:700;">$17</span><span>/hr</span></div>
                            <div class="bg-light p-2 profile-role col-lg-auto col-md-auto col-sm-auto col-auto" style="border-radius:0.5rem;">Developer</div>
                            <div class="p-2 profile-dots col-xl-auto col-auto"  style="border:solid 1px black; border-radius:8px;"><i class="fa fa-ellipsis-h"></i></div>

                        </div>
                    </div>
                </a>
            }
        </div>
    </div>
    @Html.Partial("contactPagination")
</div>



